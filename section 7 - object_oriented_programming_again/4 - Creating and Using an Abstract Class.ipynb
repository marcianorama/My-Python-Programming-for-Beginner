    "from abc import ABC, abstractmethod\n",
    "\n",
    "class AbstractAnimal(ABC):\n",
    "    @abstractmethod\n",
    "    def bark(self): pass\n",
    "\n",
    "class Dog(AbstractAnimal):\n",
    "    def bark(self):\n",
    "        print(\"Bow Bow\")\n",
    "\n",
    "print(Dog().bark())"
   ]
